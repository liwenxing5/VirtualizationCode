{ set +x
printf "%s\n" "$at_srcdir/system-offloads-traffic.at:312: ovs-appctl dpctl/dump-flows | grep \"eth_type(0x0800)\" | sed -e 's/used:\\([0-9.]*s\\|never\\)/used:0.001s/;s/eth(src=[a-z0-9:]*,dst=[a-z0-9:]*)/eth(macs)/;s/actions:[0-9,]\\+/actions:output/;s/recirc_id(0),//' | sort"
at_fn_check_prepare_notrace 'a shell pipeline' "system-offloads-traffic.at:312"
( $at_check_trace; ovs-appctl dpctl/dump-flows | grep "eth_type(0x0800)" | sed -e 's/used:\([0-9.]*s\|never\)/used:0.001s/;s/eth(src=[a-z0-9:]*,dst=[a-z0-9:]*)/eth(macs)/;s/actions:[0-9,]\+/actions:output/;s/recirc_id(0),//' | sort
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
